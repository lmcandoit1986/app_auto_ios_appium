# 弹框元素集合
# Type 为定位方式
# Value 为元素值
# 对应关系如下：
# By.NAME = name
# By.CLASS_NAME = class name
# By.ID = id
# By.XPATH = xpath
#D = "id"
#    XPATH = "xpath"
#    LINK_TEXT = "link text"
#    PARTIAL_LINK_TEXT = "partial link text"
#    NAME = "name"
#    TAG_NAME = "tag name"
#    CLASS_NAME = "class name"
#    CSS_SELECTOR = "css selector"
# permission 为系统弹框类的识别元素
# APP 为APP 自己的弹框逻辑
permission:
  - Type: id
    Value: 'cmbc close'
    ClickType: id
    ClickValue: 'cmbc close'

APP:
  - Type: id
    Value: '未知异常'
    ClickType: xpath
    ClickValue: '//XCUIElementTypeButton[@name="确定"]'

